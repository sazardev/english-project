{
  "name": "English Project - Learn English with Open Source Notes",
  "short_name": "English Project",
  "description": "A collaborative platform for English learners with resources, lessons, and community-driven content organized by levels and categories. Learn grammar, vocabulary, speaking, listening, writing, reading, pronunciation, and culture.",
  "start_url": "/",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "background_color": "#ffffff",
  "theme_color": "#3b82f6",
  "orientation": "portrait-primary",
  "scope": "/",
  "id": "english-project-app",
  "categories": ["education", "books", "productivity", "reference"],
  "lang": "en",
  "dir": "ltr",
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+englishproject",
      "url": "/share?url=%s"
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "file_handlers": [
